<view class="page">
	<view class="weui-cells weui-cells_after-title">
		<view class="weui-cell weui-cell_input">
			<view class="weui-cell__hd">
				<view class="weui-label">请输入动压(Kpa)：</view>
			</view>
			<view class="weui-cell__bd">
				<input class="weui-input" type="digit" value="{{userDonya}}" bindinput="changeDongya" />
			</view>
			<!-- <view class="weui-cell__bd warn-tail">区间：{{datas[cityIndex].shebao.min}}~{{datas[cityIndex].shebao.max}}</view> -->
		</view>
		<view class="weui-cell weui-cell_input">
			<view class="weui-cell__hd">
				<view class="weui-label">请输入静压(Kpa)：</view>
			</view>
			<view class="weui-cell__bd">
				<input class="weui-input" type="digit" value="{{userJingya}}" bindinput="changeJingya" />
			</view>
			<!-- <view class="weui-cell__bd warn-tail">区间：{{datas[cityIndex].shebao.min}}~{{datas[cityIndex].shebao.max}}</view> -->
		</view>
		<!-- <view class="weui-cell weui-cell_select">
      <view class="weui-cell__hd weui-cell__hd_in-select-after">
        <view class="weui-label">计算类型：</view>
      </view>
      <view class="weui-cell__bd">
        <picker bindchange="bindCityChange" value="{{unitIndex}}" range="{{cities}}">
          <view class="weui-select weui-select_in-select-after weui-cell__ft">{{cities[unitIndex]}}</view>
        </picker>
      </view>
    </view> -->
	</view>
	<view class="page__bd page__bd_spacing table">
		<view class="weui-flex">
			<view class="weui-flex__item">
				<view class="placeholder" style="font-weight: bold;">类型</view>
			</view>
			<view class="weui-flex__item">
				<view class="placeholder" style="font-weight: bold;">值</view>
			</view>
			<view class="weui-flex__item">
				<view class="placeholder" style="font-weight: bold;">单位</view>
			</view>
		</view>
		<block wx:for="{{datas[typeIndex].lent}}" wx:key="*this">
			<view class="weui-flex">
				<view class="weui-flex__item">
					<view class="placeholder">{{item.type}}</view>
				</view>
				<view class="weui-flex__item">
					<view class="placeholder">{{item.value}}</view>
				</view>
				<view class="weui-flex__item">
					<view class="placeholder">{{item.unit}}</view>
				</view>
			</view>
		</block>
		<view class="weui-flex">
		</view>
	</view>
	<!-- <view class="weui-btn-area">
		<button class="weui-btn" type="primary" bindtap="calculate">计算</button>
	</view> -->
	<view class="page__hd baseline-explanation">
		<view class="page__hd baseline-explanation" style="border:1px dashed #333;">
			<view class="page__desc">
				静压：运动气体的当地压力。
			</view>
			<view class="page__desc">
				动压：气流总压与静压之差。
			</view>
		</view>
	</view>
</view>